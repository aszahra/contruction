    <script>
        const elements = Array.from(document.querySelectorAll('.fill'));

        function fill(item, index) {

            const bgColor = getComputedStyle(item).backgroundColor,
                fillLayer = document.createElement('div');
            fillLayer.classList.add('fill-layer');
            item.style.backgroundColor = 'transparent';
            item.style.position = 'relative';
            item.style.overflow = 'hidden';
            setTimeout(function () {
                fillLayer.style.backgroundColor = bgColor;
                item.appendChild(fillLayer);
            }, index * 1000);

        }

        elements.forEach(fill);

        $(document).ready(function () {
            $("#abtUs").fadeOut();
            $("#srvc").fadeOut();
            $("#cp").fadeOut();
            $("#home").click(function () {
                $("#hme").fadeIn();
                $("#abtUs").fadeOut();
                $("#srvc").fadeOut();
                $("#cp").fadeOut();
            });
            $("#aboutUs").click(function () {
                $("#hme").fadeOut();
                $("#abtUs").fadeIn();
                $("#srvc").fadeOut();
                $("#cp").fadeOut();
            });
            // $("#service").click(function() {
            //     $("#hme").fadeOut();
            //     $("#abtUs").fadeOut();
            //     $("#srvc").fadeIn();
            //     $("#cp").fadeOut();
            // });            
            $("#contactPerson").click(function () {
                $("#hme").fadeOut();
                $("#abtUs").fadeOut();
                $("#srvc").fadeOut();
                $("#cp").fadeIn();
            });
        });

        $(document).ready(function () {
            $("#desain").click(function () {
                $("#dsn").fadeIn();
                $("#knstrksi").fadeOut();
                $("#mnj").fadeOut();
                $("#knsltsi").fadeOut();
            });
            $("#konstruksi").click(function () {
                $("#dsn").fadeOut();
                $("#knstrksi").fadeIn();
                $("#mnj").fadeOut();
                $("#knsltsi").fadeOut();
            });
            $("#manajemen").click(function () {
                $("#dsn").fadeOut();
                $("#knstrksi").fadeOut();
                $("#mnj").fadeIn();
                $("#knsltsi").fadeOut();
            });
            $("#konsultasi").click(function () {
                $("#dsn").fadeOut();
                $("#knstrksi").fadeOut();
                $("#mnj").fadeOut();
                $("#knsltsi").fadeIn();
            });
        });
    </script>
